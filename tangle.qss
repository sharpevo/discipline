*{
    font-family:Tamsyn;
    font-size:7pt;
    background-color: #000000;
    color:#888a85;}
QPlainTextEdit{
    selection-background-color: #000000;
    selection-color: #89BB01;
    color:#888a85;
    background: #101010;
    border:1px solid #000000;

}
QLineEdit, QSpinBox, QTextEdit{
    background:#101010;
    color: #888a85;
}
QLineEdit:focus, QSpinBox:focus, QTextEdit:focus{
    border: 1px solid #888a85;
}

/*************************************************************/
/* TreeView */
/*************************************************************/
QHeaderView::section {
    background: black;
    color: #89BB01;
}

QTreeView,  QTextBrowser{
    border: 1px solid #000000;
    background: #101010;
    alternate-background-color: #abf0ce;
    selection-background-color: #000000;
    selection-color: #89BB01;
    color: #888a85;
}
